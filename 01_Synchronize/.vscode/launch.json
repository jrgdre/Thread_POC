    "version": "0.2.0",
    "configurations": [
        {
            "name": "GDB win 64",
            "type": "by-gdb",
            "request": "launch",
            "debuggerPath": "D:\\tools\\msys64\\mingw64\\bin\\gdb.exe",
            "debuggerArgs": [
                "-quiet"
            ],
            "commandsBeforeExec": [],
            "program": "${workspaceRoot}/bin/win64/synchronize.exe",
            "programArgs": "-q -l=trace -i=SPEC.ini -o=test",
            "preLaunchTask": "x64 debug",
            "cwd": "${workspaceRoot}/bin/win64/"
        },
        {
            "name": "GDB linux 64",
            "type": "by-gdb",
            "request": "launch",
            "debuggerPath": "/usr/bin/gdb",
            "debuggerArgs": [
                "-quiet"
            ],
            "commandsBeforeExec": [],
            "program": "./bin/linux64/synchronize",
            "programArgs": "-i SPEC_2021_2020.ini",
            "preLaunchTask": "x64 debug",
            "cwd": "${workspaceRoot}"
        }
    ]
}
